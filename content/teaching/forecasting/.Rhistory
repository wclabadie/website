library(fpp3)
algeria_economy <- global_economy |>
filter(Country == "Algeria")
algeria_economy |>
autoplot(Exports) +
labs(y = "% of GDP", title = "Exports: Algeria")
View(algeria_economy)
View(algeria_economy)
fit <- algeria_economy |>
model(ETS(Exports ~ error("A") + trend("N") + season("N")))
View(fit)
fc <- fit |>
forecast(h = 5)
View(fc)
parameters(fit)
ETS(Exports ~ error("A") + trend("N") + season("N"))
View(fit)
View(fit[[2]][[1]])
fit$`ETS(Exports ~ error("A") + trend("N") + season("N"))`
fit$par
fit <- algeria_economy |>
model(ets_model = ETS(Exports ~ error("A") + trend("N") + season("N")))
fit$ets_model
fit$ets_model[[1]]
View(fit)
View(fit[[2]][[1]])
fit$ets_model[[1]]$par
View(fit)
View(fit[[2]][[1]])
fit$ets_model[[1]]$fit$par
